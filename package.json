{
  "name": "ai-review-code-rn",
  "version": "0.0.1",
  "description": "AI reviewer code RN",
  "bin": {
    "ai-review-code-rn": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/lehoi2195/ai-review-code"
  },
  "scripts": {
    "lint-fix": "eslint . --ext=js,ts,tsx --fix",
    "lint-test": "eslint . --ext=js,ts",
    "start": "ts-node ./src/index.ts",
    "review": "ts-node ./src/index.ts review",
    "test-unit": "dotenv -e .env jest",
    "publish-package": "npm i && npm run build && npm publish --access public"
  },
  "keywords": [
    "code-review",
    "rn-code-review",
    "rn-ai-code-review",
    "react-native-ai-review",
    "chatgpt",
    "openai",
    "gpt4",
    "ai",
    "llama"
  ],
  "author": "Matt Carey",
  "license": "MIT",
  "dependencies": {
    "azure-devops-node-api": "^12.3.0",
    "dotenv": "^16.3.1",
    "langchain": "^0.0.186",
    "ts-retry": "^4.2.1",
    "tslog": "^4.8.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "29.5.3",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "dotenv-cli": "^7.2.1",
    "esbuild": "0.19.1",
    "eslint": "^8.44.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "npm-dts": "^1.3.12",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/*"
  ]
}
